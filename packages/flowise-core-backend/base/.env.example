############################################################################################################
################################################ PUBLISH ###################################################
############################################################################################################

# Enable global library management for AR.js/A-Frame libraries across all publications
# When enabled, PUBLISH_DEFAULT_LIBRARY_SOURCE will set default values but user can still change them
# PUBLISH_ENABLE_GLOBAL_LIBRARY_MANAGEMENT=false

# Enforce global library management - completely blocks user choice and shows warning
# When enabled, users cannot change library sources and warning message is displayed
# For legacy spaces (created before enforcement), behavior depends on PUBLISH_AUTO_CORRECT_LEGACY_SETTINGS:
# - If auto-correction is enabled: legacy settings are automatically updated and controls are blocked
# - If auto-correction is disabled: legacy settings remain editable with recommendation messages
# PUBLISH_ENFORCE_GLOBAL_LIBRARY_MANAGEMENT=false

# Automatically correct legacy library configurations when enforcement is enabled
# When true: legacy spaces will have their library settings automatically updated to match PUBLISH_DEFAULT_LIBRARY_SOURCE
# When false: legacy spaces will show recommendation messages but remain editable when enforcement is enabled
# Only has effect when PUBLISH_ENFORCE_GLOBAL_LIBRARY_MANAGEMENT=true
# PUBLISH_AUTO_CORRECT_LEGACY_SETTINGS=true

# Default library source for AR.js and A-Frame libraries
# Options: 'official' | 'kiberplano'
# PUBLISH_DEFAULT_LIBRARY_SOURCE=kiberplano