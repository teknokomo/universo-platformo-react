{
    "name": "@flowise/agents-backend",
    "version": "0.1.0",
    "description": "Agent-specific backend services for Universo Platformo (validation, etc.)",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/cjs/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
        "lint": "eslint src --ext .ts",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "express": "^4.18.2",
        "http-status-codes": "^2.3.0",
        "typeorm": "^0.3.20",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "eslint": "^8.56.0",
        "typescript": "^5.5.4"
    },
    "license": "Apache-2.0"
}
