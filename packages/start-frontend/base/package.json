{
    "name": "@universo/start-frontend",
    "version": "0.1.0",
    "description": "Frontend module for onboarding and start page functionality",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./i18n": {
            "types": "./dist/i18n/index.d.ts",
            "import": "./dist/i18n/index.mjs",
            "require": "./dist/i18n/index.js"
        },
        "./components": {
            "default": "./src/components/index.ts"
        },
        "./components/*": {
            "default": "./src/components/*"
        },
        "./views": {
            "default": "./src/views/index.ts"
        },
        "./views/*": {
            "default": "./src/views/*"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": [
        "dist/i18n/index.mjs",
        "dist/i18n/index.js"
    ],
    "files": ["dist", "src"],
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsdown",
        "dev": "tsdown --watch",
        "lint": "eslint --ext .ts,.tsx,.jsx src/"
    },
    "keywords": [
        "universo",
        "onboarding",
        "start",
        "frontend"
    ],
    "author": "Vladimir Levadnij and Teknokomo",
    "license": "Omsk Open License",
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "catalog:",
        "@mui/material": "catalog:",
        "@universo/auth-frontend": "workspace:*",
        "@universo/utils": "workspace:*",
        "react": "catalog:",
        "react-i18next": "catalog:",
        "react-router-dom": "catalog:",
        "i18next": "catalog:",
        "typescript": "catalog:",
        "axios": "catalog:"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "react-router-dom": "^6.0.0"
    },
    "devDependencies": {
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "eslint": "^8.56.0",
        "rimraf": "^5.0.5",
        "tsdown": "catalog:"
    }
}
