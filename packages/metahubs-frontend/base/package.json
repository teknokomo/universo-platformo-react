{
    "name": "@universo/metahubs-frontend",
    "version": "0.1.0",
    "description": "Frontend module for MetaHubs management - Metadata-driven architecture",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./i18n": {
            "types": "./dist/i18n/index.d.ts",
            "import": "./dist/i18n/index.mjs",
            "require": "./dist/i18n/index.js"
        },
        "./pages/*": {
            "default": "./src/pages/*"
        },
        "./components": {
            "default": "./src/components/index.ts"
        },
        "./components/*": {
            "default": "./src/components/*"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": [
        "dist/i18n/index.mjs",
        "dist/i18n/index.js"
    ],
    "files": ["dist", "src"],
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsdown",
        "dev": "tsdown --watch",
        "lint": "eslint --ext .ts,.tsx src/"
    },
    "keywords": [
        "universo",
        "metahubs",
        "metadata",
        "frontend"
    ],
    "author": "Vladimir Levadnij and Teknokomo",
    "license": "Omsk Open License",
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/lab": "^6.0.0-beta.32",
        "@mui/material": "catalog:",
        "@tabler/icons-react": "^3.30.0",
        "@universo/auth-frontend": "workspace:*",
        "@universo/template-mui": "workspace:*",
        "react": "catalog:",
        "react-i18next": "catalog:",
        "typescript": "catalog:",
        "axios": "catalog:"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "react-router-dom": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "eslint": "catalog:",
        "rimraf": "^5.0.5",
        "tsdown": "catalog:"
    }
}
