{
    "name": "@flowise/customtemplates-frontend",
    "version": "0.1.0",
    "description": "Frontend module for Custom Templates management in Universo Platformo",
    "license": "SEE LICENSE IN LICENSE-Flowise.md",
    "homepage": "https://universo.pro",
    "author": {
        "name": "HenryHeng | Vladimir Levadnij and Teknokomo",
        "email": "info@universo.pro"
    },
    "main": "./src/index.js",
    "module": "./src/index.js",
    "exports": {
        ".": {
            "import": "./src/index.js",
            "require": "./src/index.js"
        },
        "./pages/Templates": {
            "default": "./src/pages/Templates/index.jsx"
        },
        "./pages/Templates/TemplateCanvas": {
            "default": "./src/pages/Templates/TemplateCanvas.jsx"
        },
        "./pages/Templates/TemplateCanvasHeader": {
            "default": "./src/pages/Templates/TemplateCanvasHeader.jsx"
        },
        "./pages/Templates/TemplateCanvasNode": {
            "default": "./src/pages/Templates/TemplateCanvasNode.jsx"
        },
        "./i18n": {
            "types": "./dist/i18n/index.d.ts",
            "import": "./dist/i18n/index.mjs",
            "require": "./dist/i18n/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "src",
        "dist/i18n/index.mjs",
        "dist/i18n/index.js"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "pnpm run clean && tsdown",
        "lint": "eslint --ext .js,.jsx src/",
        "lint:fix": "eslint --ext .js,.jsx src/ --fix"
    },
    "keywords": [
        "universo",
        "customtemplates",
        "templates",
        "frontend"
    ],
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "react-redux": ">=8",
        "react-router-dom": "^6.0.0",
        "reactflow": ">=11",
        "@mui/material": ">=5",
        "@tabler/icons-react": ">=2",
        "@universo/api-client": "workspace:*",
        "@universo/i18n": "workspace:*",
        "@universo/spaces-frontend": "workspace:*",
        "@flowise/template-mui": "workspace:*",
        "@flowise/store": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "catalog:",
        "@universo/i18n": "workspace:*",
        "eslint": "catalog:",
        "rimraf": "^5.0.5",
        "tsdown": "catalog:",
        "typescript": "catalog:"
    }
}
