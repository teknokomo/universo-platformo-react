{
    "name": "@universo/template-mui",
    "version": "0.1.0",
    "description": "Material-UI template implementation for Universo Platformo React",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./components/table/FlowListTable": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./components/dialogs": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./components/dialogs/EntityFormDialog": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "NODE_OPTIONS='--max-old-space-size=8192' tsdown && NODE_OPTIONS='--max-old-space-size=8192' tsc --emitDeclarationOnly",
        "dev": "tsdown --watch",
        "lint": "eslint --ext .ts,.tsx,.jsx src/",
        "test": "NODE_OPTIONS='--require ./jest-canvas-mock.js' jest",
        "test:coverage": "NODE_OPTIONS='--require ./jest-canvas-mock.js' jest --coverage",
        "test:watch": "NODE_OPTIONS='--require ./jest-canvas-mock.js' jest --watch"
    },
    "dependencies": {
        "@flowise/store": "workspace:*",
        "@flowise/template-mui": "workspace:*",
        "@flowise/apikey-frontend": "workspace:*",
        "@flowise/assistants-frontend": "workspace:*",
        "@flowise/credentials-frontend": "workspace:*",
        "@flowise/customtemplates-frontend": "workspace:*",
        "@flowise/docstore-frontend": "workspace:*",
        "@flowise/executions-frontend": "workspace:*",
        "@flowise/tools-frontend": "workspace:*",
        "@flowise/variables-frontend": "workspace:*",
        "@tanstack/react-query": "^5.62.13",
        "@mui/material": "catalog:",
        "@mui/icons-material": "catalog:",
        "@mui/x-charts": "catalog:",
        "@mui/x-data-grid": "catalog:",
        "@mui/x-data-grid-pro": "catalog:",
        "@mui/x-date-pickers": "catalog:",
        "@mui/x-date-pickers-pro": "catalog:",
        "@mui/x-tree-view": "catalog:",
        "@universo/i18n": "workspace:*",
        "@universo/types": "workspace:*",
        "@universo/utils": "workspace:*",
        "@universo/admin-frontend": "workspace:*",
        "@universo/start-frontend": "workspace:*",
        "@emotion/react": "catalog:",
        "@emotion/styled": "catalog:",
        "dayjs": "catalog:",
        "html-react-parser": "^5.1.10",
        "use-debounce": "^10.0.6",
        "react-hook-form": "catalog:",
        "@hookform/resolvers": "catalog:",
        "react-redux": "catalog:"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "eslint": "catalog:",
        "rimraf": "^5.0.5",
        "tsdown": "catalog:",
        "typescript": "catalog:",
        "@testing-library/react": "catalog:",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/user-event": "catalog:",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "ts-jest": "^29.2.5",
        "identity-obj-proxy": "^3.0.0"
    },
    "typesVersions": {
        "*": {
            "components/dialogs": [
                "dist/components/dialogs/index.d.ts"
            ],
            "components/dialogs/EntityFormDialog": [
                "dist/components/dialogs/EntityFormDialog.d.ts"
            ],
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "sideEffects": [
        "*.css"
    ],
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
            "react-router-dom": "catalog:"
    },
    "keywords": [
        "universo",
        "mui",
        "material-ui",
        "template",
        "react"
    ],
    "author": "Universo Platformo Team",
    "license": "MIT"
}
