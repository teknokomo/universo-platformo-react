{
  "name": "@universo/spaces-frt",
  "version": "0.1.0",
  "description": "Frontend module for Spaces/Canvases (UI extraction)",
  "private": false,
  "keywords": [
    "universo",
    "spaces",
    "frontend"
  ],
  "license": "Omsk Open License",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./i18n": {
      "import": "./dist/i18n/index.mjs",
      "require": "./dist/i18n/index.js"
    },
    "./views/*": "./src/views/*",
    "./entry/*": "./src/entry/*",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/entry",
    "src/views"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "vitest run --config vitest.config.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@flowise/chatmessage-frt": "workspace:*",
    "@flowise/customtemplates-frt": "workspace:*",
    "@flowise/docstore-frt": "workspace:*",
    "@universo/i18n": "workspace:*",
    "@universo/utils": "workspace:*"
  },
  "peerDependencies": {
    "@universo/auth-frt": ">=0.1.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "catalog:",
    "react-redux": "^8.0.5",
    "react-router-dom": "catalog:",
    "@mui/material": "5.15.0",
    "@tabler/icons-react": "^3.30.0",
    "react-perfect-scrollbar": "^1.5.8"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.3",
    "@universo/auth-frt": "workspace:*",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.23",
    "rimraf": "^5.0.5",
    "tsdown": "catalog:",
    "typescript": "^5.4.5",
    "vitest": "^2.1.9"
  }
}
