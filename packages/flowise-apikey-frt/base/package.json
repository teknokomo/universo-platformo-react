{
  "name": "@universo/flowise-apikey-frt",
  "version": "0.1.0",
  "description": "ApiKey management frontend package for Universo Platformo",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./pages/*": {
      "default": "./src/pages/*"
    },
    "./i18n": {
      "types": "./src/i18n/index.ts",
      "import": "./src/i18n/index.ts",
      "require": "./src/i18n/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": ["src"],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts,.tsx,.jsx src/"
  },
  "keywords": [
    "universo",
    "flowise",
    "apikey",
    "frontend"
  ],
  "author": "Teknokomo",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": "^6.0.0",
    "@mui/material": ">=5",
    "@universo/api-client": "workspace:*",
    "@universo/i18n": "workspace:*",
    "@flowise/template-mui": "workspace:*",
    "@flowise/store": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "rimraf": "^5.0.5",
    "typescript": "catalog:"
  }
}
