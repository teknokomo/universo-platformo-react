{
    "name": "@flowise/credentials-frontend",
    "version": "0.1.0",
    "description": "Frontend module for Credentials management in Universo Platformo",
    "license": "SEE LICENSE IN LICENSE-Flowise.md",
    "homepage": "https://universo.pro",
    "author": {
        "name": "HenryHeng | Vladimir Levadnij and Teknokomo",
        "email": "info@universo.pro"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./pages/*": {
            "default": "./src/pages/*"
        },
        "./i18n": {
            "types": "./src/i18n/index.ts",
            "import": "./src/i18n/index.ts",
            "require": "./src/i18n/index.ts"
        },
        "./package.json": "./package.json"
    },
    "files": ["src"],
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint --ext .ts,.tsx,.jsx src/"
    },
    "keywords": [
        "universo",
        "credentials",
        "frontend"
    ],
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "react-router-dom": "^6.0.0",
        "@mui/material": ">=5",
        "@universo/api-client": "workspace:*",
        "@universo/i18n": "workspace:*",
        "@flowise/template-mui": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "eslint": "catalog:",
        "rimraf": "^5.0.5",
        "typescript": "catalog:"
    }
}
