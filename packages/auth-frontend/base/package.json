{
        "name": "@universo/auth-frontend",
        "version": "0.1.0",
        "description": "Shared authentication UI primitives for Universo Platformo",
        "private": true,
        "main": "dist/index.js",
        "module": "dist/index.mjs",
        "types": "dist/index.d.ts",
        "exports": {
                ".": {
                        "types": "./dist/index.d.ts",
                        "import": "./dist/index.mjs",
                        "require": "./dist/index.js"
                },
                "./pages/AuthPage": {
                        "types": "./dist/pages/AuthPage.d.ts",
                        "import": "./dist/pages/AuthPage.mjs",
                        "require": "./dist/pages/AuthPage.js"
                }
        },
        "engines": {
                "node": ">=18.15.0 <19 || >=20"
        },
        "scripts": {
                "clean": "rimraf dist",
                "build": "tsdown",
                "dev": "tsdown --watch",
                "lint": "eslint --ext .ts,.tsx src"
        },
        "dependencies": {
                "@universo/utils": "workspace:*",
                "axios": "catalog:"
        },
        "peerDependencies": {
                "@mui/material": ">=7",
                "@mui/system": ">=7",
                "@mui/icons-material": ">=7",
                "@tabler/icons-react": "^3.30.0",
                "@yandex/smart-captcha": "catalog:",
                "react": "^18.2.0",
                "react-dom": "^18.2.0",
                "react-router-dom": "catalog:"
        },
        "peerDependenciesMeta": {
                "@yandex/smart-captcha": {
                        "optional": true
                }
        },
        "devDependencies": {
                "@mui/material": "catalog:",
                "@mui/system": "catalog:",
                "@mui/icons-material": "catalog:",
                "@tabler/icons-react": "^3.30.0",
                "@types/react": "^18.3.3",
                "@types/react-dom": "^18.3.2",
                "react": "catalog:",
                "react-dom": "catalog:",
                "rimraf": "^5.0.5",
                "tsdown": "catalog:",
                "typescript": "catalog:"
        }
}
